{
    "type": "mssql",
    "host": "{{HOST}}",
    "username": "{{USERNAME}}",
    "password": "{{PASSWORD}}",
    "database": "{{DATABASE}}",
    "synchronize": false,
    "entities":["dist/**/*.entity.js"],
    "migrations": ["dist/migrations/*.js"],
    "cli": {
        "migrationsDir": "migrations"
    },
    "options": {
        "encrypt": false,
        "enableArithAbort": true
    }
}